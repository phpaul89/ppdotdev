{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": false,
        "quotemark": [
            true,
            "double",
            "jsx-double"
        ],
        "interface-name": [
            false
        ],
        "jsx-no-multiline-js": [
            false
        ]
    },
    "rulesDirectory": []
}